<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>mypackage.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admincheck</servlet-name>
        <servlet-class>mypackage.admincheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout1</servlet-name>
        <servlet-class>mypackage.Logout1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgetpass</servlet-name>
        <servlet-class>mypackage.forgetpass</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admincheck</servlet-name>
        <url-pattern>/admincheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout1</servlet-name>
        <url-pattern>/Logout1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgetpass</servlet-name>
        <url-pattern>/forgetpass</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
